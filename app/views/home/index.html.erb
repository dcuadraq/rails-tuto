<% @questions.each do |q| %>
  <div class="media">
    <div class="media-left">
      <a href="#">
        <img class="media-object" src="http://thenextweb.com/wp-content/blogs.dir/1/files/2010/03/gravatar.jpg" alt="Not found">
      </a>
    </div>
    <div class="media-body">
      <h4 class="media-heading"><%= q.email %></h4>
      <%= q.body %>
      <div>
        <a href="/questions/12" class="btn btn-success btn-xs">View Answers</a>
      </div>
    </div>
  </div>
<% end %>
